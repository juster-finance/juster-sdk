{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "declaration": true,
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "module": "esnext",
      "moduleResolution": "node",
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "sourceMap": true
    },
    "exclude": [
      "node_modules",
      "dist",
      "build"
    ]
}  