{
  "appName": "Juster",
  "ratioPrecision": "100000000",
  "providerProfitFee": "0.1",
  "networks": {
    "testnet": {
      "contractAddress": "KT1JPehwEkAhg1z5RVA8wAccYbDgF2E2itDv",
      "rpcNode": "https://rpc.tzkt.io/granadanet/",
      "graphqlUri": "https://api.granadanet.juster.fi/v1/graphql",
      "subscriptionUri": "wss://api.granadanet.juster.fi/v1/graphql"
    },
    "mainnet": {
      "contractAddress": "",
      "rpcNode": "",
      "graphqlUri": "",
      "subscriptionUri": ""
    }
  },
  "entrypoints": {
    "withdraw": {
      "prim": "pair",
      "args": [
        {
          "prim": "nat",
          "annots": [
            "%eventId"
          ]
        },
        {
          "prim": "address",
          "annots": [
            "%participantAddress"
          ]
        }
      ]
    },
    "provideLiquidity": {
      "prim": "pair",
      "args": [
        {
          "prim": "pair",
          "args": [
            {
              "prim": "nat",
              "annots": [
                "%eventId"
              ]
            },
            {
              "prim": "nat",
              "annots": [
                "%expectedRatioAboveEq"
              ]
            }
          ]
        },
        {
          "prim": "nat",
          "annots": [
            "%expectedRatioBelow"
          ]
        },
        {
          "prim": "nat",
          "annots": [
            "%maxSlippage"
          ]
        }
      ]
    },
    "bet": {
      "prim": "pair",
      "args": [
        {
          "prim": "pair",
          "args": [
            {
              "prim": "or",
              "args": [
                {
                  "prim": "unit",
                  "annots": [
                    "%aboveEq"
                  ]
                },
                {
                  "prim": "unit",
                  "annots": [
                    "%below"
                  ]
                }
              ],
              "annots": [
                "%bet"
              ]
            },
            {
              "prim": "nat",
              "annots": [
                "%eventId"
              ]
            }
          ]
        },
        {
          "prim": "mutez",
          "annots": [
            "%minimalWinAmount"
          ]
        }
      ]
    }
  }
}
